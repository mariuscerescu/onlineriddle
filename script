function answer1() {
  var input = document.getElementById("txtbox").value;
  if(input === "awesome"){

document.getElementById("form1").action="awesome.html";
  } else {
    document.getElementById("wrong").innerHTML ="The answer is wrong!"
  }
}


function validateMyForm1() {
var input = document.getElementById("txtbox").value;
  if(input === "awesome"){

return true;
  } else {
    
return false;
  }

};


// -------------------------------------------------------------m----------------------------------
	function disappear() {
		document.getElementById("container").innerHTML= "";
		document.getElementById('message').innerHTML = 'Ha Ha Ha <br> Now try to find it!'
        document.getElementById('counter').innerHTML = 1
        document.getElementById('btn2').style.display = "inline"


	}

function disappear2(){

		document.getElementById('message').innerHTML = 'Congratulations! <br> But now you have to press the button 100 times!'
        document.getElementById('counter').innerHTML = 2
     document.getElementById('btn2').style.display = "none"
    document.getElementById('ptb').innerHTML = "Press the button 100 times"
     document.getElementById('btn4').style.display="inline"


}
	
function disappear3(){

	var i = document.getElementById("counter").innerHTML;
	if (i<100){
		i++;
	} else{
		i = 0
	}

if(i === 100){

	document.getElementById('message').innerHTML = 'Ha Ha Ha <br> I tricked you! <br> You will have to do it forever'
}

if(i === 15){

	document.getElementById('btn4').style.color ="grey"
    document.getElementById('btn4').textContent ="Find now"
     document.getElementById('btn5').style.display="inline"
} else {
	document.getElementById('btn4').style.color ="black"
	document.getElementById('btn4').textContent ="Press Me"
	document.getElementById('btn5').style.display="none"
}



	document.getElementById("counter").innerHTML = i;

}

function disappear4(){
document.getElementById('btn5').style.display ="none"
document.getElementById('btn4').style.display ="none"
document.getElementById('table').style.display="inline"
document.getElementById('message').innerHTML = 'You are going well <br> But my last challenge for you will be impossible!<br> <br>Make all the buttons disappear <br> Good Luck!'
document.getElementById('counter').style.display ="none"
document.getElementById('ptb').style.display ="none"
document.getElementById('reset').style.display ="inline"
}


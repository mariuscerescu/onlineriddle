// -------------------------------------------------form validation-------------------------------
function validation() {
  var form = document.getElementById("form");
  var email = document.getElementById("email").value;
  var text = document.getElementById("validationText");
  var pattern =/^[^\s@]+@[^\s@]+\.[^\s@]+$/;

  if (email.match(pattern)) {
    form.classList.add("valid")
    form.classList.remove("invalid")
    text.innerHTML = "Your Email Address is Valid."
    text.style.color = "#00ff00"
    document.getElementById("form").action = "contact-form.php"; 
    document.getElementById("form").onsubmit = "return true"; 
  } else{
    form.classList.remove("valid")
    form.classList.add("invalid")
    text.innerHTML = "Please Enter a Valid Email Address."
    text.style.color = "#ff0000"
    document.getElementById("form").action = "javascript:void(0)";
  }
  if (email == "") {
    form.classList.remove("valid")
    form.classList.remove("invalid")
    text.innerHTML = ""
    text.style.color = "#00ff00"
    document.getElementById("form").action = "javascript:void(0)";

  }
}

